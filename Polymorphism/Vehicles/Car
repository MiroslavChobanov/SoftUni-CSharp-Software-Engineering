using System;
using System.Collections.Generic;
using System.Text;

namespace Vehicles
{
    public class Car : Vehicle
    {
        private const double AirConConsumption = 0.9;
        public Car(double fuelConsumption, double fuelQuantity) : base(fuelConsumption, fuelQuantity)
        {

        }

        public override double FuelConsumption => base.FuelConsumption + AirConConsumption;
    }
}
